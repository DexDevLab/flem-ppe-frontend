{
  "name": "flem-ppe-frontend",
  "description": "Aplicação Frontend para o Projeto Primeiro Emprego, da FLEM",
  "private": true,
  "version": "v1.1.0-230623",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "docs": "jsdoc --readme README.md -c jsdoc-config.json",
    "db-push:dev": "dotenv -e .env.development -- yarn prisma db push",
    "db-seed:dev": "dotenv -e .env.development -- yarn prisma db seed",
    "studio:dev": "dotenv -e .env.development -- yarn prisma studio",
    "docker:build": "docker build . -t ghcr.io/frtechdev/flem-ppe-frontend:latest",
    "docker:push": "docker push ghcr.io/frtechdev/flem-ppe-frontend:latest",
    "caniuse": "npm update --save caniuse-lite browserslist",
    "clean": "yarn autoclean --force",
    "git:clear": "bash cleargitcache.sh"
  },
  "dependencies": {
    "@chakra-ui/icons": "1.1.1",
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/theme-tools": "^2.0.18",
    "@deevotechvn/quill-mention": "^3.1.3",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11",
    "activedirectory": "0.7.2",
    "apexcharts": "^3.33.1",
    "axios": "^0.26.0",
    "chakra-react-select": "^4.1.3",
    "clean-jsdoc-theme": "^4.2.9",
    "downloadjs": "^1.4.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^6",
    "google-libphonenumber": "^3.2.27",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "masks-br": "^1.0.1",
    "next": "12.1.6",
    "next-auth": "^4.22.1",
    "npm-check": "^6.0.1",
    "pdfjs-dist": "^3.7.107",
    "quill": "^1.3.7",
    "react": "18.1.0",
    "react-apexcharts": "^1.3.9",
    "react-currency-format": "^1.1.0",
    "react-dom": "18.1.0",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.3.1",
    "react-no-ssr": "^1.1.0",
    "react-pdf": "^7.1.2",
    "react-quilljs": "^1.2.17",
    "react-spinners": "^0.13.8",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "jsdoc": "^3.6.10",
    "prisma": "^4.1.0"
  }
}
